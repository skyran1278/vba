{
  // DATA TYPES ===================================================
  /*
    Boolean
    Long
    Double
    String
    Object
    Array
    */
  "Boolean": {
    "prefix": "DimBoolean",
    "body": [
      "Dim bol$1 As Boolean"
    ],
    "description": "Boolean declaration"
  },
  "Long": {
    "prefix": "DimLong",
    "body": [
      "Dim lng$1 As Long"
    ],
    "description": "Long declaration"
  },
  "Double": {
    "prefix": "DimDouble",
    "body": [
      "Dim dbl$1 As Double"
    ],
    "description": "Double declaration"
  },
  "String": {
    "prefix": "DimString",
    "body": [
      "Dim str$1 As String"
    ],
    "description": "String declaration"
  },
  "Object": {
    "prefix": "DimObject",
    "body": [
      "Dim obj$1 As Object"
    ],
    "description": "Object declaration"
  },
  "Array": {
    "prefix": "DimArray",
    "body": [
      "Dim arr$1()"
    ],
    "description": "Array declaration"
  },
  "Private": {
    "prefix": "Private",
    "body": [
      "Private "
    ],
    "description": "Private declaration"
  },
  // If Else ===================================================
  "If": {
    "prefix": "If",
    "body": [
      "If ${1:condition} Then",
      "\t$2",
      "End If"
    ],
    "description": "If code block"
  },
  "If Else": {
    "prefix": "IfElse",
    "body": [
      "If ${1:condition1} Then",
      "\t$2",
      "Else",
      "\t$3",
      "End If"
    ],
    "description": "If Else code block"
  },
  "If ElseIf": {
    "prefix": "IfElseIf",
    "body": [
      "If ${1:condition1} Then",
      "\t$2",
      "ElseIf ${3:condition2} Then",
      "\t$4",
      "Else",
      "\t$4",
      "End If"
    ],
    "description": "If ElseIf Else code block"
  },
  // With ===================================================
  "With": {
    "prefix": "With",
    "body": [
      "With $1",
      "\t$2",
      "End With"
    ],
    "description": "With code block"
  },
  // Loops ===================================================
  "For Loop": {
    "prefix": "for",
    "body": [
      "For ${1:index} = ${3:lower} To ${4:upper}",
      "\t$0",
      "Next ${1:index}"
    ],
    "description": "For Next Loop"
  },
  "ForEach": {
    "prefix": "ForEach",
    "body": [
      "For Each ${1:variable} In ${2:collection}",
      "\t$0",
      "Next ${1:variable}"
    ],
    "description": "For Each"
  },
  "DoLoopWhile": {
    "prefix": "DoLoopWhile",
    "body": [
      "Do",
      "\t$0",
      "Loop While ${1:condition}"
    ],
    "description": "Do Loop While code block"
  },
  "DoWhile": {
    "prefix": "DoWhile",
    "body": [
      "Do While ${1:condition}",
      "\t$0",
      "Loop"
    ],
    "description": "Do While Loop code block"
  },
  // Sub Function ===================================================
  "SubWithComments": {
    "prefix": "SubWithComments",
    "body": [
      "Sub Main()",
      "'",
      "' @purpose:",
      "' $1",
      "'",
      "'",
      "' @algorithm:",
      "' $2",
      "'",
      "'",
      "' @test:",
      "' $3",
      "'",
      "'",
      "",
      "\t$0",
      "",
      "End Sub"
    ],
    "description": "Sub code block with comments"
  },
  "Sub": {
    "prefix": "Sub",
    "body": [
      "Sub ${1:subName}()",
      "'",
      "'",
      "'",
      "' @param",
      "' @returns",
      "",
      "\t$0",
      "",
      "End Sub"
    ],
    "description": "Sub code block"
  },
  "Function": {
    "prefix": "Function",
    "body": [
      "Function ${1:functionName}()",
      "'",
      "'",
      "'",
      "' @param",
      "' @returns",
      "",
      "\t$0",
      "",
      "End Function"
    ],
    "description": "Function code block"
  },
  "SelectCase": {
    "prefix": "SelectCase",
    "body": [
      "Select Case ${1:test}",
      "",
      "\tCase ${2:lists}",
      "",
      "\t\t${3:statements}",
      "",
      "\tCase Else",
      "",
      "\t\t${0:elseStatement}",
      "",
      "End Select"
    ],
    "description": "Select Case code block"
  },
  "MsgBox": {
    "prefix": "MsgBox",
    "body": [
      "MsgBox(\"${1:message}\", ${2:buttonType}, \"${3:title}\")"
    ],
    "description": "Message box code block"
  }
}
