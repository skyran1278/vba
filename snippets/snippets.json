  // If Else ===================================================
  "If": {
    "prefix": "If",
    "body": [
      "If ${1:condition} Then",
      "\t$2",
      "End If"
    ],
    "description": "If code block"
  },
  "If Else": {
    "prefix": "IfElse",
    "body": [
      "If ${1:condition1} Then",
      "\t$2",
      "Else",
      "\t$3",
      "End If"
    ],
    "description": "If Else code block"
  },
  "If ElseIf": {
    "prefix": "IfElseIf",
    "body": [
      "If ${1:condition1} Then",
      "\t$2",
      "ElseIf ${3:condition2} Then",
      "\t$4",
      "Else",
      "\t$4",
      "End If"
    ],
    "description": "If ElseIf Else code block"
  },
  "ElseIf": {
    "prefix": "ElseIf",
    "body": [
      "ElseIf ${1:condition2} Then",
      "$0"
    ],
    "description": "ElseIf"
  },
  // With ===================================================
  "With": {
    "prefix": "With",
    "body": [
      "With $1",
      "\t$2",
      "End With"
    ],
    "description": "With code block"
  },
  // Loops ===================================================
  "For Loop": {
    "prefix": "for",
    "body": [
      "For ${1:i} = ${3:lower} To ${4:upper}",
      "\t$0",
      "Next ${1:i}"
    ],
    "description": "For Next Loop"
  },
  "ForEach": {
    "prefix": "ForEach",
    "body": [
      "For Each ${1:variable} In ${2:collection}",
      "\t$0",
      "Next ${1:variable}"
    ],
    "description": "For Each"
  },
  "DoLoopWhile": {
    "prefix": "DoLoopWhile",
    "body": [
      "Do",
      "\t$0",
      "Loop While ${1:condition}"
    ],
    "description": "Do Loop While code block"
  },
  "DoWhile": {
    "prefix": "DoWhile",
    "body": [
      "Do While ${1:condition}",
      "\t$0",
      "Loop"
    ],
    "description": "Do While Loop code block"
  },
  "While": {
    "prefix": "While",
    "body": [
      "While ${1:condition}",
      "\t$0",
      "Wend"
    ],
    "description": "While Wend code block"
  },
  // Sub Function ===================================================
  "Sub": {
    "prefix": "Sub",
    "body": [
      "Private Sub ${1:func}(${2:input})",
      "'",
      "' ${3:descrip}.",
      "'",
      "' @since ${4:1.0.0}",
      "' @param {${5:type}} [${6:name}] ${7:descrip}.",
      "' @return {${8:type}} [${9:name}] ${10:descrip}.",
      "' @see ${11:dependencies}",
      "'",
      "",
      "\t$0",
      "",
      "End Sub"
    ],
    "description": "Sub code block"
  },
  "Function": {
    "prefix": "Function",
